<div id="Match">

  <div class="md-content">
    <h1>Picture to Match</h1>
    <div id="fields">

      <%= form_for @picture, url: user_house_match_path, html: {multipart: true} do |f| %>

      <p><%= f.file_field :image, type: :file, multiple: true %></p>

      <p><%= f.submit 'Send pictures', class: "send_picture"%></p>
      <% end %>
      <br>

      <p><%= link_to 'back to profile', controller: 'users', action: 'dashboard', class:"links" %></p>
    </div>
  </div>

</div>
